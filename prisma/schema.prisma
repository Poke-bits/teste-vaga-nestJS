generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Product {
  id    String   @id @default(auto()) @map("_id") @db.ObjectId
  name  String
  price Float
  sku   String   @unique
  createdAt DateTime @default(now())
  deletedAt DateTime? 
}
